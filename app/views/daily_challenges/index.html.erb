<div class="container">

<p style="color: green"><%= notice %></p>

<div class="row tasks mb-3">
  <h1>Daily challenges</h1>
</div>

<div id="daily_challenges">
  <% @daily_challenges.each do |daily_challenge| %>
    <%= daily_challenge.user.username %>
    <%= daily_challenge.date %>
    <%= link_to "Show", daily_challenge %>

  <% end %>
</div>

</div>